{
  "name": "wmf-nest",
  "version": "0.1.0",
  "private": true,
  "description": "Server application for Winnipeg Music Festival online forms",
  "author": "David Sawatzky",
  "license": "UNLICENSED",
  "scripts": {
    "migrate:dev": "dotenvx run --env-file=.env.development -- prisma migrate dev",
    "seed:dev": "dotenvx run --env-file=.env.development -- prisma db seed",
    "prebuild": "rimraf dist",
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "prisma generate --watch",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start --watch -b swc --type-check",
    "start:swc": "cross-env NODE_ENV=development nest start --watch -b swc --type-check",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "testvi": "cross-env NODE_ENV=test vitest run",
    "testvi:watch": "cross-env NODE_ENV=test vitest --watch --ui",
    "testvi:cov": "cross-env NODE_ENV=test vitest --run --coverage",
    "testvi:debug": "cross-env NODE_ENV=test vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "testvi:e2e": "cross-env NODE_ENV=test vitest --watch --config ./vitest.config.e2e.mts",
    "testvi:e2e:watch": "cross-env NODE_ENV=test LOG_LEVEL=silent vitest --watch --config ./vitest.config.e2e.mts",
    "testvi:e2e:ui": "cross-env NODE_ENV=test LOG_LEVEL=silent vitest --watch --ui --config ./vitest.config.e2e.mts",
    "types": "ts-node ./src/generate-typings"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.2",
    "@dotenvx/dotenvx": "^1.51.0",
    "@jest/globals": "^30.2.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/devtools-integration": "^0.2.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@prisma/client": "^6.17.0",
    "@sentry/nestjs": "^10.21.0",
    "@sentry/vite-plugin": "^4.5.0",
    "@types/nodemailer": "^7.0.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "mjml": "^4.16.1",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.17.0",
    "prisma-graphql-type-decimal": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "serverless-http": "^4.0.0",
    "stripe": "^18.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@nestjs/schematics": "^11.0.8",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "^24.7.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "supertest-graphql": "^1.1.4",
    "ts-loader": "^9.5.4",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "unplugin-swc": "^1.5.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.2.4"
  },
  "prisma": {
    "seed": "bash prisma/seed.sh"
  }
}
