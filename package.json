{
  "name": "wmf-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "prisma generate --watch",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:swc": "cross-env Node_ENV=development nest start -b swc --type-check",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=development jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "test:cov": "cross-env NODE_ENV=development jest --coverage",
    "test:debug": "cross-env NODE_ENV=development node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=development jest --config ./test/jest-e2e.json",
    "testvi": "vitest run",
    "testvi:watch": "vitest",
    "testvi:cov": "vitest run --coverage",
    "testvi:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "testvi:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "types": "ts-node ./src/generate-typings"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@casl/ability": "^6.5.0",
    "@casl/prisma": "^1.4.1",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@prisma/client": "^5.4.2",
    "@types/nodemailer": "^6.4.11",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^8.2.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "handlebars": "^4.7.8",
    "mjml": "^4.14.1",
    "nodemailer": "^6.9.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-graphql-type-decimal": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "serverless-http": "^3.2.0",
    "stripe": "^13.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.92",
    "@swc/jest": "^0.2.29",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.19",
    "@types/jest": "29.5.3",
    "@types/node": "^18.18.4",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/coverage-v8": "^0.34.6",
    "esbuild": "^0.19.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "helmet": "^7.0.0",
    "jest": "29.6.2",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "supertest-graphql": "^1.1.4",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "unplugin-swc": "^1.4.3",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "globalSetup": "<rootDir>/test/setup.ts",
    "globalTeardown": "<rootDir>/test/teardown.ts",
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest",
        {
          "astTransformers": {
            "before": [
              "./src/test/astTransformer.ts"
            ]
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
